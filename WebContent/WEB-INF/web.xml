<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>ListJNDI</servlet-name>
    <servlet-class>ListJNDI</servlet-class>
  </servlet> 
<servlet>
    <servlet-name>lookupDS</servlet-name>
    <servlet-class>LookupDS</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ListJNDI</servlet-name>
    <url-pattern>/servlet/ListJNDI</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>lookupDS</servlet-name>
    <url-pattern>/servlet/LookupDS</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
